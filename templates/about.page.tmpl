{{template "base" .}}

{{define "content"}}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1>This is the about page two</h1>
                <p>This came from the template: {{index .StringMap "test"}}</p>
            <p>    
                {{ if ne (index .StringMap "remote_ip") "" }}
                    Your remote IP is: {{ index .StringMap "remote_ip" }}
                {{else}}
                    I dont know your IP address yet, visit the <a href="/">Home page</a>
                {{end}}
            </p>
            </div>
        </div>
    </div>
{{end}}
